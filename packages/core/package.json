    "peerDependencies": {
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@swc/core": "1.3.96",
        "@swc/helpers": "0.5.3",
        "@swc/jest": "0.2.29",
        "@testing-library/react": "14.0.0",
        "@types/jest": "29.5.7",
        "@types/react": "18.2.36",
        "@types/react-dom": "18.2.14",
        "@workleap/eslint-plugin": "3.0.0",
        "@workleap/swc-configs": "2.1.2",
        "@workleap/tsup-configs": "3.0.1",
        "@workleap/typescript-configs": "3.0.2",
        "jest": "29.7.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "ts-jest": "29.1.1",
        "tsup": "7.2.0",
        "typescript": "5.2.2"
