{
    "name": "@squide/core",
    "author": "Workleap",
    "version": "3.0.0",
    "description": "The core package of @squide federated application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/core"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
