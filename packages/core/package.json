{
    "name": "@squide/core",
    "author": "Workleap",
    "version": "5.4.1",
    "description": "The core package of @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/core"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "jest"
    },
    "peerDependencies": {
        "react": "*"
    },
    "dependencies": {
        "@types/react": "19.0.1",
        "eventemitter3": "5.0.1"
    },
    "devDependencies": {
        "@rsbuild/plugin-react": "1.1.0",
        "@rslib/core": "0.1.3",
        "@swc/core": "1.10.1",
        "@swc/jest": "0.2.37",
        "@types/jest": "29.5.14",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "react": "19.0.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
