{
    "name": "@squide/firefly-configs",
    "author": "Workleap",
    "version": "1.0.0",
    "description": "Webpack configuration helpers for the Squide firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
