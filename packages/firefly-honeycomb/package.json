{
    "name": "@squide/firefly-honeycomb",
    "author": "Workleap",
    "version": "2.0.2",
    "description": "Honeycomb instrumentation for the Squide firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/firefly-honeycomb"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "jest"
    },
    "peerDependencies": {
        "@honeycombio/opentelemetry-web": "*",
        "@opentelemetry/api": "*",
        "@opentelemetry/auto-instrumentations-web": "*",
        "@opentelemetry/instrumentation-fetch": "*",
        "@opentelemetry/sdk-trace-web": "*"
    },
    "dependencies": {
        "@squide/firefly": "workspace:*",
        "@types/uuid": "10.0.0",
        "@workleap/honeycomb": "1.0.0",
        "uuid": "11.0.5"
    },
    "devDependencies": {
        "@rsbuild/core": "1.1.13",
        "@rslib/core": "0.3.1",
        "@swc/core": "1.10.7",
        "@swc/jest": "0.2.37",
        "@types/jest": "29.5.14",
        "@typescript-eslint/parser": "8.20.0",
        "@workleap/eslint-plugin": "3.2.6",
        "@workleap/rslib-configs": "1.0.2",
        "@workleap/swc-configs": "2.2.4",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
