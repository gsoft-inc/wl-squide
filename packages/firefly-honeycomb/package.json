{
    "name": "@squide/firefly-honeycomb",
    "author": "Workleap",
    "version": "0.0.1",
    "description": "Honeycomb instrumentation for the Squide firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/firefly-honeycomb"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "dev": "tsup --config ./tsup.dev.ts",
        "build": "tsup --config ./tsup.build.ts"
    },
    "peerDependencies": {
        "@honeycombio/opentelemetry-web": "*",
        "@opentelemetry/api": "*",
        "@opentelemetry/auto-instrumentations-web": "*"
    },
    "devDependencies": {
        "@opentelemetry/instrumentation": "*",
        "@opentelemetry/instrumentation-document-load": "*",
        "@opentelemetry/instrumentation-fetch": "*",
        "@opentelemetry/instrumentation-user-interaction": "*",
        "@opentelemetry/instrumentation-xml-http-request": "*",
        "@opentelemetry/sdk-trace-web": "*",
        "@swc/core": "1.7.26",
        "@swc/jest": "0.2.36",
        "@types/jest": "29.5.13",
        "@types/uuid": "10.0.0",
        "@workleap/eslint-plugin": "3.2.2",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/tsup-configs": "3.0.6",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "ts-jest": "29.2.5",
        "tsup": "8.3.0",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "@squide/firefly": "workspace:*",
        "uuid": "10.0.0"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=20.0.0"
    }
}
