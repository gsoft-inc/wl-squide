{
    "name": "@squide/firefly-rsbuild-configs",
    "author": "Workleap",
    "version": "0.0.1",
    "description": "Rsbuild configuration helpers for the Squide firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/firefly-rsbuild-configs"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "dev": "rslib build --watch --config ./rslib.dev.ts",
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "jest"
    },
    "peerDependencies": {
        "@rsbuild/core": "*",
        "@rspack/core": "*"
    },
    "dependencies": {
        "@module-federation/enhanced": "0.8.1",
        "@module-federation/rsbuild-plugin": "0.8.1",
        "@rsbuild/plugin-basic-ssl": "1.1.1",
        "@rsbuild/plugin-react": "1.1.0",
        "@rsbuild/plugin-svgr": "1.0.6",
        "@types/node": "22.10.1",
        "@types/semver": "7.5.8",
        "deepmerge": "4.3.1",
        "semver": "7.6.3"
    },
    "devDependencies": {
        "@rsbuild/core": "1.1.9",
        "@rslib/core": "0.1.3",
        "@rspack/core": "1.1.5",
        "@swc/core": "1.10.1",
        "@swc/jest": "0.2.37",
        "@types/jest": "29.5.14",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "engines": {
        "node": ">=21.1.0"
    }
}
