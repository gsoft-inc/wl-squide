{
    "name": "@squide/firefly",
    "author": "Workleap",
    "version": "9.3.1",
    "description": "Helpers to facilitate the creation of an application with the Squide firefly technology stack.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/firefly"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "jest"
    },
    "peerDependencies": {
        "@tanstack/react-query": "*",
        "msw": "*",
        "react": "*",
        "react-dom": "*",
        "react-router-dom": "*"
    },
    "dependencies": {
        "@squide/core": "workspace:*",
        "@squide/module-federation": "workspace:*",
        "@squide/msw": "workspace:*",
        "@squide/react-router": "workspace:*",
        "@types/react": "19.0.1",
        "@types/react-dom": "19.0.2"
    },
    "devDependencies": {
        "@rsbuild/plugin-react": "1.1.0",
        "@rslib/core": "0.1.3",
        "@swc/core": "1.10.1",
        "@swc/jest": "0.2.37",
        "@testing-library/jest-dom": "6.6.3",
        "@testing-library/react": "16.1.0",
        "@types/jest": "29.5.14",
        "@typescript-eslint/parser": "8.18.0",
        "@workleap/eslint-plugin": "3.2.3",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "msw": "2.6.8",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-router-dom": "6.27.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
