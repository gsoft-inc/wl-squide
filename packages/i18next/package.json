{
    "name": "@squide/i18next",
    "author": "Workleap",
    "version": "1.1.4",
    "description": "Add support for i18next to @squide federated application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/i18next"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "dev": "tsup --config ./tsup.dev.ts",
        "build": "tsup --config ./tsup.build.ts"
    },
    "peerDependencies": {
        "i18next": "*",
        "i18next-browser-languagedetector": "*",
        "react": "*",
        "react-dom": "*",
        "react-i18next": "*"
    },
    "devDependencies": {
        "@swc/core": "1.4.13",
        "@swc/jest": "0.2.36",
        "@types/jest": "29.5.12",
        "@types/react": "18.2.77",
        "@types/react-dom": "18.2.25",
        "@workleap/eslint-plugin": "3.2.2",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/tsup-configs": "3.0.6",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "i18next": "23.11.1",
        "i18next-browser-languagedetector": "7.2.1",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-i18next": "14.1.0",
        "ts-jest": "29.1.2",
        "tsup": "8.0.2",
        "typescript": "5.4.5"
    },
    "dependencies": {
        "@formatjs/intl-localematcher": "0.5.4",
        "@squide/core": "workspace:*"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=20.0.0"
    }
}
