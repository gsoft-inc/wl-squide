{
    "name": "@squide/i18next",
    "author": "Workleap",
    "version": "2.2.1",
    "description": "Add support for i18next to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/i18next"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "jest"
    },
    "peerDependencies": {
        "i18next": "*",
        "i18next-browser-languagedetector": "*",
        "react": "*",
        "react-i18next": "*"
    },
    "dependencies": {
        "@formatjs/intl-localematcher": "0.5.10",
        "@squide/core": "workspace:*",
        "@types/react": "19.0.7"
    },
    "devDependencies": {
        "@rsbuild/core": "1.1.13",
        "@rslib/core": "0.3.1",
        "@swc/core": "1.10.7",
        "@swc/jest": "0.2.37",
        "@types/jest": "29.5.14",
        "@typescript-eslint/parser": "8.20.0",
        "@workleap/eslint-plugin": "3.2.5",
        "@workleap/swc-configs": "2.2.3",
        "@workleap/rslib-configs": "1.0.0",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "i18next": "24.2.1",
        "i18next-browser-languagedetector": "8.0.2",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "react": "19.0.0",
        "react-i18next": "15.4.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
