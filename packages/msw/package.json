{
    "name": "@squide/msw",
    "author": "Workleap",
    "version": "3.2.1",
    "description": "Add support for MSW to @squide application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gsoft-inc/wl-squide.git",
        "directory": "packages/msw"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "peerDependencies": {
        "msw": "*"
    },
    "peerDependenciesMeta": {
        "msw": {
            "optional": true
        }
    },
    "dependencies": {
        "@squide/core": "workspace:*"
    },
    "devDependencies": {
        "@rsbuild/core": "1.1.13",
        "@rslib/core": "0.3.1",
        "@typescript-eslint/parser": "8.20.0",
        "@workleap/eslint-plugin": "3.2.5",
        "@workleap/rslib-configs": "1.0.0",
        "@workleap/typescript-configs": "3.0.2",
        "eslint": "8.57.0",
        "msw": "2.7.0",
        "typescript": "5.5.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
