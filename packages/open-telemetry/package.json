{
    "name": "open-telemetry",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Add support for Open Telemetry to @squide federated application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/open-telemetry"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./configTransformer.js": "./dist/configTransformer.js"
    },
    "types": "./dist/index.d.ts",
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "dev": "tsup --config ./tsup.dev.ts",
        "build": "tsup --config ./tsup.build.ts"
    },
    "dependencies": {
        "@opentelemetry/api": "^1.4.1",
        "@opentelemetry/context-zone": "^1.13.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.39.1",
        "@opentelemetry/instrumentation": "^0.39.1",
        "@opentelemetry/instrumentation-fetch": "^0.39.1",
        "@opentelemetry/instrumentation-xml-http-request": "^0.39.1",
        "@opentelemetry/resources": "^1.13.0",
        "@opentelemetry/sdk-trace-web": "^1.13.0",
        "@opentelemetry/semantic-conventions": "^1.13.0"
    },
    "devDependencies": {
        "@swc/core": "1.3.56",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/jest": "^29.5.1",
        "@workleap/eslint-plugin": "1.6.0",
        "@workleap/typescript-configs": "2.3.0",
        "jest": "29.5.0",
        "ts-node": "10.9.1",
        "tsup": "6.7.0"
    }
}
