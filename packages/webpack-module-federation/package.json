{
    "name": "@squide/webpack-module-federation",
    "author": "Workleap",
    "version": "1.0.0",
    "description": "Add support for Webpack Module Federation to @squide federated application shell.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-squide.git",
        "directory": "packages/webpack-module-federation"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./configTransformer.js": {
            "import": "./dist/configTransformer.js",
            "types": "./dist/configTransformer.d.ts",
            "default": "./dist/configTransformer.js"
        }
    },
    "files": [
        "/dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "dev": "tsup --config ./tsup.dev.ts",
        "build": "tsup --config ./tsup.build.ts"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom": "*",
        "webpack": ">=5.0.0"
    },
    "devDependencies": {
        "@swc/core": "1.3.66",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/jest": "29.5.2",
        "@types/react": "18.2.13",
        "@types/react-dom": "18.2.6",
        "@workleap/eslint-plugin": "1.8.2",
        "@workleap/swc-configs": "1.0.0",
        "@workleap/tsup-configs": "2.0.0",
        "@workleap/typescript-configs": "2.3.2",
        "jest": "29.5.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "tsup": "7.0.0",
        "typescript": "5.0.4",
        "webpack": "5.88.0"
    },
    "dependencies": {
        "@squide/core": "workspace:*",
        "deepmerge": "4.3.1"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=18.0.0"
    }
}
