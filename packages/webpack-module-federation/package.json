    },
    "devDependencies": {
        "@swc/core": "1.3.62",
        "@swc/helpers": "0.5.1",
        "@swc/jest": "0.2.26",
        "@types/jest": "29.5.2",
        "@types/react": "18.2.9",
        "@types/react-dom": "18.2.4",
        "@workleap/eslint-plugin": "1.8.1",
        "@workleap/typescript-configs": "2.3.1",
        "jest": "29.5.0",
        "npm-check-updates": "16.10.12",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "ts-node": "10.9.1",
        "ts-jest": "29.1.0",
